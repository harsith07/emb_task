#include <AT89S52.h> 
#define ULED P2_0
#define switch P3_7

void delay(int);
void main()
{


int n;
 switch=1;
 ULED =1;
while(1){
 if(switch==0){
 for(n=0;n<5;n++){
  ULED=1;
  delay(100);
  ULED=0;
  delay(100);
 }
 }
 else
 {
 ULED=0;
 }
 }
}

 void delay(int time) {
 int i,j;
 for(i=0;i<time;i++)
 {
  for(j=0;j<1275;j++)
  {}
 }
 }
